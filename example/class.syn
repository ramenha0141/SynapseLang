class A {
    value: int;
    constructor(initial_value: int) {
        this.value = initial_value;
    }
    add(value: int) {
        this.value = this.value + value;
    }
    get(): int {
        return this.value;
    }
}
function main() {
    const a: A = new A(2);
    a.add(3);
    console::puti(a.get());
    delete a;
}